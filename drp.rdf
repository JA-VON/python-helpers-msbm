<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac11">
	<skos:prefLabel xml:lang="en">Adaptation</skos:prefLabel>
	<dct:created rdf:datatype="xsd:http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:created>
	<hasStatus xmlns="http://art.uniroma2.it/ontologies/vocbench#">Proposed</hasStatus>
	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:modified>
	<skos:definition xml:lang="en">The adjustment in natural or human systems in response to actual or expected climatic stimuli or their effects, which moderates harm or exploits beneficial opportunities. </skos:definition>
</rdf:Description>


<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac12">
