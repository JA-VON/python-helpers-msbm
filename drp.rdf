<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac10">
	<skos:prefLabel xml:lang="en">Acceptable risk</skos:prefLabel>
	<dct:created rdf:datatype="xsd:http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:created>
	<hasStatus xmlns="http://art.uniroma2.it/ontologies/vocbench#">Proposed</hasStatus>
	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:modified>
	<skos:definition xml:lang="en">The level of potential losses that a society or community considers acceptable given existing social, economic, political, cultural, technical and environmental conditions. </skos:definition>
</rdf:Description>


