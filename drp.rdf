<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac165">
	<skos:prefLabel xml:lang="en">Local Magnitude (ML) AKA Richter</skos:prefLabel>
	<dct:created rdf:datatype="xsd:http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:created>
	<hasStatus xmlns="http://art.uniroma2.it/ontologies/vocbench#">Proposed</hasStatus>
	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:modified>
	<skos:definition xml:lang="en">Scale used for Small/Local Scale Earthquake Measurement. Great 8 or more. Major 7 - 7.9. Strong 6 - 6.9. Moderate 5 - 5.9. Light 4 - 4.9. Minor 3 -3.9</skos:definition>
</rdf:Description>


