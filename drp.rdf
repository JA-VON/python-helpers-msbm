</rdf:Description>

<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac13">
	<skos:prefLabel xml:lang="en">Building code</skos:prefLabel>
	<dct:created rdf:datatype="xsd:http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:created>
	<hasStatus xmlns="http://art.uniroma2.it/ontologies/vocbench#">Proposed</hasStatus>
	<dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2016-03-31T04:32:05Z</dct:modified>
	<skos:definition xml:lang="en">A set of ordinances or regulations and associated standards intended to control aspects of the design, construction, materials, alteration and occupancy of structures that are necessary to ensure human safety and welfare, including resistance to collapse and damage. </skos:definition>
</rdf:Description>


<rdf:Description rdf:about="http://vocab.cdmk-caribbean.net/cdmk#c_893a08ac14">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<skos:inScheme rdf:resource="http://vocab.cdmk-caribbean.net/cdmk"/>
